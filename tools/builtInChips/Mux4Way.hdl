/** 
 * Multiplexor:
 out = a if sel == 00
 *     b if sel == 01
 *     c if sel == 10
 *     d if sel == 11
 */
 CHIP Mux4Way {

    IN  a, b, c, d, sel[2];
    OUT out;

    BUILTIN Mux4Way;
}
